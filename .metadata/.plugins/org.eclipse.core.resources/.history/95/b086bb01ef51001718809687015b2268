 .global fibonacci
;r0 is n
;mov r0, #2

CHECK_0
    cmp r0, #0
    beq TOP
CHECK_1
	cmp R0, #1
	beq TOP
SAVE
    push {lr}
    ;sub sp, sp, #4
    ;str lr, [sp, #4]
TOP
    cmp r0, #0
    beq L_ZERO
    cmp r0, #1
    beq ONE
    bgt RECURSE

L_ZERO
	mov r0, #1
	push {r0}
	;sub sp, sp, #4
	;str r0, [sp, #4]
	BL TOP
ONE
	mov r0, #1
	push {r0}
	;sub sp, sp, #4
	;str r0, [sp, #4]
	BL TOP
RECURSE
	;get fib -1
	sub r0,r0, #1
	BL TOP

	;get fib -2
	sub r0, r0, #1
    bl TOP
	pop {r8}
	pop {r9}
	add r0, r8, r9
	push {r0}
EXIT
    pop {r0}
    pop {lr}
    mov pc, lr




